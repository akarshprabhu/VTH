
@{
    ViewData["Title"] = "Leaderboard";
}
<h1>@ViewData["Title"]</h1>

<table class="table table-striped">
    <tr>
        <th>Question No.</th>
        <th>Player</th>
    </tr>
    @foreach (var d in Model)
    {
        <tr>
            <td>@d.Qno</td>
            <td>@d.UserId</td>
        </tr>
    }
</table>
